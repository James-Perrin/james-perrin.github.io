<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
    <h2>Hear</h2>
    <ul>
    <% for (var i=0; i<results[0].length; i++){ %>
        <li><%= results[0][i].hear %> :  <%= results[0][i].count %></li>
    <% } %>    
    </ul>

    <h2>Single</h2>
    <ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].single %> :  <%= results[1][i].count %></li>
    <% } %>    
    </ul>
    <h2>Frequency</h2>
    <ul>

    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].frequency %> :  <%= results[2][i].count %></li>
    <% } %>    
    </ul>

    <h2>Links</h2>
    <ul>
    <% for (var i=0; i<results[3].length; i++){ %>
        <li><%= results[3][i].links %> :  <%= results[3][i].count %></li>
    <% } %>    
    </ul>

    <h2>Feel</h2>
    <ul>
    <% for (var i=0; i<results[4].length; i++){ %>
        <li><%= results[4][i].feel %> :  <%= results[4][i].count %></li>
    <% } %>    
    </ul>
    
    <h2>Additional</h2>
    <ul>
    <% for (var i=0; i<results[5].length; i++){ %>
        <li><%= results[5][i].additional %> :  <%= results[5][i].count %></li>
    <% } %>    
    </ul>
</body>
</html>